.containers .nav {
  width: 100%;
  height: var(--status-bar-height);
  background-color: #FFFFFF;
  z-index: 999;
}
.containers .container .status_bar {
  height: var(--status-bar-height);
  width: 100%;
}
.containers .container .sticky-box {
  position: -webkit-sticky;
  position: sticky;
  top: var(--status-bar-height);
  z-index: 99;
}
.containers .container .return-img {
  width: 42rpx;
  height: 42rpx;
}
.containers .container .input {
  width: 626rpx;
  height: 60rpx;
  border-radius: 30rpx;
}
.containers .container .back-img {
  height: 540rpx;
}
.containers .container .view-text {
  width: 550rpx;
  height: 100rpx;
  border: 2rpx solid #F54A35;
}

