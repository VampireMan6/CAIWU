<view class="container p-b-20 b-b"><view class="top relative"><swiper class="w-100 h-100 m-b-10" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}" current="{{swiperCurrent}}" data-event-opts="{{[['change',[['changeSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperImg}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><image class="swiper-item w-100 h-100 " mode="aspectFill" src="{{item.imgs}}"></image></swiper-item></block></swiper><view class="nav-b absolute p-20 b-b"><image class="return-img _img" src="{{$root.m0}}" alt data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><image class="share-img _img" src="{{$root.m1}}" alt></image></view><view class="number-list absolute z-999 f-15 c-white f-w-600"><text>{{list[swiperCurrent]}}</text><text>{{"/"+swiperImg.length}}</text></view></view><view class="w-100 company-message p-x-15 b-b relative"><view class="w-100 view b-r-2 b-c-w column"><block wx:if="{{datas1.data[id].name.length<16}}"><text class="f-20 c-333 m-l-15 m-t-12">{{datas1.data[id].name}}</text></block><block wx:else><text class="f-20 c-333 m-l-15 m-t-12">{{$root.g0+'...'}}</text></block><view class="w-100 f-between"><text class="f-13 c-999 m-l-15 m-t-5">{{"数据更新"+datas1.data[id].time}}</text><view class="right h-100 f-center j-center f-10 b-r-2"><text>+ 公司对比</text></view></view></view></view><view class="w-100 p-x-30 b-b f-15 c-333 column"><text class="m-b-10">{{datas1.data[id].title}}</text><text class="f-13 c-999 m-b-5">{{datas1.data[id].tel}}</text><text class="f-13 c-999 m-b-5">{{datas1.data[id].email}}</text><text class="f-13 c-999 m-b-10">{{datas1.data[id].address}}</text><view class="border-1 m-b-10"></view><view class="w-100 f-13 m-b-10"><text>上证A股</text><text class="m-l-25">行业名称</text><text class="m-l-25">小会计准则</text></view></view><view class="border-3"></view><block wx:for="{{datas2}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="w-100 column statement">